{
  "name": "express-app-server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "serve": "tsx ./dist/src/index.js",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json && babel ./dist --out-dir dist --extensions \".js\" --copy-files --keep-file-extension",
    "start": "tsx --tsconfig tsconfig.json src/index.ts",
    "dev": "tsx watch --tsconfig tsconfig.json src/index.ts",
    "lint": "eslint --fix . && prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
